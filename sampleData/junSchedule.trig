    a schema:Pet ;
    schema:name "Fluffy" ;
    schema:animalType "Cat" ;
    schema:medicalHistory "Fluffy had a minor surgery last year." ;
  ], [
    a schema:Pet ;
    schema:name "Buddy" ;
    schema:animalType "Dog" ;
    schema:medicalHistory "Buddy is up to date on vaccinations." ;
  ] .
}

