@prefix schema: <http://schema.org/> .
@prefix ex: <http://example.org/> .

ex:jun belief:believes ex:nigel .

[]
  acp:grant acl:Read, acl:Write ;  
  acp:context [
    acp:agent ex:nigel ;
    acp:target <http://example.org/jun/events> ;
  ] .

