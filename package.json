  "name": "phd-language-dialogue-experiment",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "interface:cli": "ts-node humanInterfaces/run",
    "build": "tsc",
    "ldo": "ldo build --input ./shapes --output ./ldo && ldo build --input ./shapes/proofCheckers --output ./ldo/proofCheckers",
    "shapes": "shacl2shex shapes/ && npm run ldo && rm -rf ./shapes/*.shex && rm -rf ./shapes/**/*.shex",
    "watch:shapes": "nodemon --watch shapes/*.shaclc -x \"npm run shapes\"",
    "watch:tsc": "tsc --watch",
    "watch": "run-p watch:*",
    "build:components": "componentsjs-generator ./trust -s ./trust/dist",
    "serve:interface:nigel": "ts-node humanInterfaces/run.ts -p 3008 -w http://localhost:3002/nigel#me -s http://localhost:3006/ -i cli",
    "serve:interface:jun": "ts-node humanInterfaces/run.ts -p 3007 -w http://localhost:3001/jun#me -s http://localhost:3005/ -i cli",
    "serve:interface": "ttab -t \"Nigel\" -d ./ -w npm run serve:interface:nigel && ttab -t \"Jun\" -d ./ -w npm run serve:interface:jun",
    "serve:agentServer:nigel": "ts-node agentServers/server.ts -p 3006 -w http://localhost:3002/nigel#me -s http://localhost:3008/ -u sampleData/nigelSchedule.trig -k sampleData/nigelKey.json",
    "serve:agentServer:jun": "ts-node agentServers/server.ts -p 3005 -w http://localhost:3001/jun#me -s http://localhost:3007/ -u sampleData/junSchedule.trig -k sampleData/junKey.json",
    "serve:agentServers": "run-p -l serve:agentServer:*",
    "serve:webIds:nigel": "rdf-serve sampleData/webIds/nigel -p 3002",
    "serve:webIds:jun": "rdf-serve sampleData/webIds/jun -p 3001",
    "serve:webIds": "run-p -l serve:webIds:*",
    "serve": "run-p serve:*",
    "secret:set": "gh secret set -f .env"
  },
